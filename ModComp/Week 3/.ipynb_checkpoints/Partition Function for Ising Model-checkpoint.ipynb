   "source": [
    "\n",
    "def Z(N, w = 1):\n",
    "    s = 0\n",
    "    for i in tqdm(range(2**N)):\n",
    "        chain = 2*np.array(list(np.binary_repr(i,N)), dtype=int) - 1\n",
    "        s += np.exp(-w*chain[1:]*chain[:-1]).sum()\n",
    "    return s \n",
    "            \n",
    "Z(10)\n",
    "\n"
   ]
  }
