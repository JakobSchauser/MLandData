    "\n",
    "def div(n, sofar = \"\"):\n",
    "    if(n == 0):\n",
    "        return sofar[::-1]\n",
    "    sofar += str(n%2)\n",
    "    print(n%2)\n",
    "    return div(n//2,sofar)\n",
